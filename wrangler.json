{
	"$schema": "node_modules/wrangler/config-schema.json",
	"compatibility_date": "2024-11-18",
	"main": "./build/server/index.js",
	"name": "instagres",
	"workers_dev": false,
	"observability": { "logs": { "enabled": true } },
	"assets": {
		"directory": "./build/client/"
	},
	"triggers": {
		"crons": ["*/5 * * * *"]
	},
	"unsafe": {
		"bindings": [
			{
				"name": "RATE_LIMITER",
				"type": "ratelimit",
				"namespace_id": "1001",
				"simple": {
					"limit": 100,
					"period": 60
				}
			}
		]
	}
}
